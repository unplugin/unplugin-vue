{
  "name": "unplugin-vue",
  "version": "0.1.0",
  "description": "Plugin for Vue 3",
  "keywords": [
    "vue",
    "sfc",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest",
    "build": "tsup",
    "release": "standard-version"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue/issues"
  },
  "homepage": "https://github.com/sxzz/unplugin-vue#readme",
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^4.1.2",
    "@sxzz/eslint-config-prettier": "^1.6.1",
    "@sxzz/eslint-config-ts": "^1.6.2",
    "@types/hash-sum": "^1.0.0",
    "@types/node": "*",
    "eslint": "^8.7.0",
    "eslint-define-config": "^1.2.2",
    "fast-glob": "^3.2.11",
    "hash-sum": "^2.0.0",
    "querystring": "^0.2.1",
    "slash": "^4.0.0",
    "source-map": "^0.6.1",
    "standard-version": "^9.3.2",
    "tsup": "^5.11.11",
    "typescript": "^4.5.4",
    "unplugin": "^0.3.0",
    "vite": "^2.7.13",
    "vitest": "^0.1.23",
    "vue": "^3.2.27"
  }
}
